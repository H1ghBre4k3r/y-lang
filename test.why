struct Point {
    x: i64;
    y: i64;
}

declare sqrt: (i64) -> f64;

fn distance(p1: Point, p2: Point): f64 {
    let dx: i64 = p1.x - p2.x;
    let dy: i64 = p1.y - p2.y;
    sqrt((dx * dx + dy * dy))
}

fn main(): void {
    let origin: Point = Point {
        x: 0,
        y: 0
    };
    let point: Point = Point {
        x: 3,
        y: 4
    };
    let dist: f64 = distance(origin, point);
    println("Distance: {}", dist);
}
