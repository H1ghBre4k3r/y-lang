fn main(): i64 {
    let add_one = makeAdder(1);
    let multiply_by_3 = makeMultiplier(3);
    let complex = complexCapture(10, 20);

    add_one(42) + multiply_by_3(5) + complex()
}

fn makeAdder(x: i64): (i64) -> i64 {
    \(y) => x + y
}
fn makeMultiplier(factor: i64): (i64) -> i64 {
    \(value) => factor * value
}
fn complexCapture(a: i64, b: i64): () -> i64 {
    \() => a * b + 100
}
